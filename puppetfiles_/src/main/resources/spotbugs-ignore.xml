<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Package name="~.*?generated.*"/>
    </Match>
    <Match>
        <Class name="~.*Test*"/>
    </Match>
    <Match>
       <Bug pattern="CRLF_INJECTION_LOGS" />
    </Match>
    <Match> <!-- In my experience in Java 11 the compiler is generating byte code that causes false positives on this rule. -->
       <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE" />
    </Match>
    <Match> <!-- In my experience in Java 11 the compiler is generating byte code that causes false positives on this rule. -->
       <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
    </Match>
</FindBugsFilter>